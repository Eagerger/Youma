<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="com" extends="struts-default">
<action name="login" class="com.UserAction" method="login">
<result name="loginout">/loginerror.jsp</result>
<result name="loginin">/index.jsp</result>
</action>
<action name="regist" class="com.UserAction" method="regist">
<result name="error">/loginerror.jsp</result>
<result name="success">/login.jsp</result>
</action>
<action name="checkfirmdata" class="com.UserAction" method="checkfirmdata">
<result name="error">/completefirmif.jsp</result>
<result name="success">/login.jsp</result>
</action>
<action name="checkpersondata" class="com.UserAction" method="checkfirmdata">
<result name="error">/completepsif.jsp</result>
<result name="success">/login.jsp</result>
</action>
<action name="completefirm" class="com.UserAction" method="completefirm">
<result name="error">/loginerror.jsp</result>
<result name="success">/testsuccess.jsp</result>
</action>
<action name="completeperson" class="com.UserAction" method="completeperson">
<result name="error">/loginerror.jsp</result>
<result name="success">/testsuccess.jsp</result>
</action>
</package>
</struts>